# Input data
salaries <- c(70000, 78000, 62000, 66000, 61000, 72000,
58000, 64000, 60000, 73000, 74000, 76000)
mu0 <- 73000  # Hypothesized population mean
n <- length(salaries)  # Sample size (n = 12)
# Calculate sample statistics
xbar <- mean(salaries)  # Sample mean
S <- sd(salaries)       # Sample standard deviation (use sd() instead of sqrt(var()))
SE <- S / sqrt(n)       # Standard error of the mean
# Compute t-statistic
t <- (xbar - mu0) / SE  # t ≈ -2.1909 (negative due to x̄ < μ₀)
# Calculate p-value (left-tailed test: H1: μ < 73000)
df <- n - 1  # Degrees of freedom = 11
p_value <- pt(t, df, lower.tail = TRUE)  # p ≈ 0.0255
# Decision rule (α = 0.05)
if (p_value < 0.05) {
cat("Reject null hypothesis (p =", round(p_value, 4), ")\n")
cat("Conclusion: Average salary is significantly BELOW 73,000 INR.\n")
} else {
cat("Fail to reject null hypothesis (p =", round(p_value, 4), ")\n")
}
