heads_tails <- function() {
Heads_Tails <- seq(-3, 3, 2)
k <- c(3, 1, 1, 3)
Prob <- dbinom(k, 3, 0.5)
data.frame(Heads_Tails, Prob)
}
df <- heads_tails()
#(a) P(0<=x<=3)
p3 <- sum(df$Prob[df$Heads_Tails >= 0])
#(b) P(x>3)
p4 <- sum(df$Prob[df$Heads_Tails > 3])
print(df)
print(paste("P(0 <= x <= 3):", p3))
print(paste("P(0 > 3):", p4))
print(paste("P(x > 3):", p4))
